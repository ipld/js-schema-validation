  "license": "(Apache-2.0 AND MIT)",
  "devDependencies": {
    "@ipld/block": "^4.0.2",
    "hundreds": "0.0.2",
    "ipld-schema": "^0.3.2",
    "mocha": "^7.1.2",
    "polendina": "^1.0.0",
    "standard": "^14.3.3"
  },
  "dependencies": {
    "bytesish": "^0.4.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipld/js-schema-validation.git"
  },
  "bugs": {
