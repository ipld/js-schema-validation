{
  "name": "@ipld/schema-validation",
  "version": "0.0.0-dev",
  "description": "Validate decoded data against an IPLD Schema",
  "main": "index.js",
  "scripts": {
    "pretest": "standard",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha test/test-*.js",
    "coverage": "nyc --reporter=html mocha test/test-*.js && npx st -d coverage -p 8080 --index=true",
    "test:browser": "polendina --cleanup test/test-*.js"
  },
